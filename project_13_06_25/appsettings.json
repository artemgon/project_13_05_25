{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "UdpConfig": {
    "Port": 11000,
    "LocalIP": "0.0.0.0",
    "TimeoutMs": 2000,
    "BufferSize": 1024,
    "EnableBroadcast": false,
    "MulticastGroup": null,
    "AllowLoopback": true,
    "MaxConnections": 100,
    "EnableStatistics": true
  }
}